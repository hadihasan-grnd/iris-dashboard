  <script>
    // Small JS for interactions: sidebar active, join button animation, card clicks, mobile toggle

    // Add click listeners to nav items (toggle active)
    document.querySelectorAll('.nav-item').forEach(item => {
      item.addEventListener('click', (e) => {
        document.querySelectorAll('.nav-item').forEach(i => i.classList.remove('active'));
        item.classList.add('active');
        // demo: scroll to related section (best-effort mapping)
        const key = item.dataset.key;
        if(key === 'announcement') window.scrollTo({top:400, behavior:'smooth'});
        else if(key === 'attendance') window.scrollTo({top:760, behavior:'smooth'});
        else if(key === 'courses') window.scrollTo({top:1150, behavior:'smooth'});
        else if(key === 'timetable') window.scrollTo({top:1740, behavior:'smooth'});
        else window.scrollTo({top:0, behavior:'smooth'});
      });
    });

    // Join button pulse on click + toast-like demo
    const joinBtn = document.getElementById('joinBtn');
    joinBtn.addEventListener('click', () => {
      joinBtn.animate([
        { transform: 'scale(1)' },
        { transform: 'scale(1.06)' },
        { transform: 'scale(1)' }
      ], { duration: 350, easing: 'ease-out' });

      // quick temporary notification
      const n = document.createElement('div');
      n.textContent = 'You have joined Launchpad 25 ‚Äî check Announcements for updates.';
      Object.assign(n.style, {
        position: 'fixed', right: '18px', bottom: '18px',
        padding: '12px 16px', background:'#222', color:'#fff', borderRadius:'10px',
        boxShadow: '0 10px 40px rgba(0,0,0,0.6)', zIndex:9999, fontWeight:600
      });
      document.body.appendChild(n);
      setTimeout(()=> n.style.opacity = '0', 2200);
      setTimeout(()=> n.remove(), 2600);
    });

    // Click handlers on clickable cards just show simple highlight and console log (dummy interactions)
    document.querySelectorAll('.clickable').forEach(el => {
      el.addEventListener('click', () => {
        el.animate([
          { transform: 'translateY(0px)' },
          { transform: 'translateY(-6px)' },
          { transform: 'translateY(0px)' }
        ], { duration: 220, easing: 'ease-out' });
        console.log('Clicked:', el.dataset.title || (el.querySelector('.course-code')?.textContent || 'card'));
      });
    });

    // Make attendance status indicators colored appropriately based on parent classes
    document.querySelectorAll('.attendance-grid > .att-card').forEach(card => {
      const dot = card.querySelector('.status-indicator');
      if(card.classList.contains('danger')) dot.style.background = 'linear-gradient(90deg,#ff6b6b,#ff3b3b)';
      else if(card.classList.contains('warning')) dot.style.background = 'linear-gradient(90deg,#ffd166,#ffb703)';
      else dot.style.background = 'linear-gradient(90deg,#48d66b,#2ea84a)';
    });

    // Make timetable horizontally scrollable with small drag support (desktop)
    (function enableDragScroll(el){
      let isDown=false, startX, scrollLeft;
      el.addEventListener('mousedown', (e)=>{
        isDown=true; el.classList.add('active'); startX = e.pageX - el.offsetLeft; scrollLeft = el.scrollLeft;
      });
      el.addEventListener('mouseleave', ()=>{isDown=false; el.classList.remove('active')});
      el.addEventListener('mouseup', ()=>{isDown=false; el.classList.remove('active')});
      el.addEventListener('mousemove', (e)=>{
        if(!isDown) return;
        e.preventDefault();
        const x = e.pageX - el.offsetLeft;
        const walk = (x - startX) * 1.6;
        el.scrollLeft = scrollLeft - walk;
      });
    })(document.getElementById('ttCards'));

    // Accessibility: keyboard activation for clickable cards
    document.querySelectorAll('.clickable[tabindex]').forEach(el=>{
      el.addEventListener('keydown', (e) => { if(e.key === 'Enter' || e.key === ' ') el.click(); });
    });

    // Responsive: toggling sidebar on small screens when tapping the logo
    const sidebar = document.getElementById('sidebar');
    document.querySelector('.logo').addEventListener('click', () => {
      if(window.innerWidth < 700){
        sidebar.classList.toggle('collapsed');
      }
    });

    // Close collapsed sidebar when clicking outside on small screens
    document.addEventListener('click', (e) => {
      if(window.innerWidth < 700){
        if(!sidebar.contains(e.target) && sidebar.classList.contains('collapsed')){
          sidebar.classList.remove('collapsed');
        }
      }
    });

    // Small touch: make current-class visible on load (scroll to it)
    setTimeout(()=> {
      const cur = document.querySelector('.tt-card.current-class');
      if(cur) cur.scrollIntoView({behavior:'smooth', inline:'center'});
    }, 600);
  </script>
</body>
</html>
